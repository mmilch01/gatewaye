<vocabulary>
<!--
Note: xnat_column_alias is a vocabulary-defined key that is used internally for 
entry indexing. It should be unique throughout the vocabulary.
 -->

<!-- PatientID -->
<entry dicom_group="0010" dicom_element="0020" xnat_element_name="xnat:subjectData" 
		xnat_field_id="xnat:subjectData/ID" xnat_header="PatientID" search_column_alias="patid" >
	<criterion search_schema_field="xnat:subjectData/ID" method="OR" comparison_type="LIKE"/>
</entry>

<!-- Patient Name -->
<entry dicom_group="0010" dicom_element="0010" xnat_element_name="xnat:subjectData"
		xnat_field_id="xnat:subjectData/label" xnat_header="PatientName" search_column_alias="patname">
	<criterion search_schema_field="xnat:subjectData/label" method="OR" comparison_type="LIKE"/>
</entry>

<!-- Patient Sex -->
<entry dicom_group="0010" dicom_element="0040" xnat_element_name="xnat:subjectData"
		xnat_field_id="xnat:demographicData/gender" xnat_header="PatientSex" search_column_alias="patsex">
	<criterion search_schema_field="xnat:subjectData/gender" method="OR" comparison_type="LIKE"/>		
	<value_map dcm_val="M" xnat_val="male"/>
	<value_map dcm_val="F" xnat_val="female"/>
</entry>

<!-- Patient Birth Date -->
